CREATE DATABASE movies_db;

SELECT * FROM movies;

CREATE TABLE user (
    username VARCHAR(255) PRIMARY KEY,
    password VARCHAR(255)

);

CREATE TABLE movies (
    id INT AUTO_INCREMENT PRIMARY KEY,
    title VARCHAR(255),
    duration INT,
    language VARCHAR(50),
    director VARCHAR(255),
    mainActor VARCHAR(255),
    min_age INT,
    id_owner INT

);


